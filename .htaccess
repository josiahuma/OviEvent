# .htaccess for ovievent.com
<IfModule mod_rewrite.c>
RewriteEngine On

# Force everything on ovievent.com (with or without www) to eventib.com
RewriteCond %{HTTP_HOST} ^ovievent\.com$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www\.ovievent\.com$ [NC]
RewriteRule ^(.*)$ https://eventib.com/$1 [R=301,L,QSA]
</IfModule>
